<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/assets/css/admin.css">
    <title>CRM || Teacher Page</title>
</head>
<body>

  <div class="container">
    <div class="header">
      <div class="logo-section">
        <i class="fas fa-crown"></i>
        <i class="fas fa-chevron-down"></i>
      </div>
      <div class="search-section">
        <div class="search-box">
          <input type="text" class="input-box">
          <i class="fas fa-search"></i>
        </div>
        <span class="date">06 May 2020</span>
      </div>
      <div class="profile-section">
          <!--  -->
      </div>
    </div>
    <div class="header-two">
      <div class="request-section">
        <button class="request-button">Request for</button>
      </div>
      <div class="people-header">
        <div class="people-header-left">
            <!-- <% myTeacher.slice(2,3).map(i => { %> -->
                <h2><%= i.name %></h2>
            <!-- <% }) %> -->
          <ul>
            <li class="active">All</li>
            <li>Organization</li>
          </ul>
        </div>
        <div class="people-header-right">
          <div class="search-box">
            <input type="text" placeholder="Search by name" class="input-box">
            <i class="fas fa-search"></i>
          </div>
        </div>
      </div>
      <div class="selected-team">
          
      </div>
    </div>
    <div class="content-section">
      <div class="nav-bar">
        <ul class="nav-items">
          <li class="nav-item">
            <i class="fas fa-columns"></i>
            <span>Guruhlar</span>
          </li>
        </ul>
        <button class="sign-out">
          <i class="fas fa-power-off"></i>
        </button>
      </div>
      <div class="main-content">
        
        <div class="user-cards">

            <% groups.map(e => { %>
                <h5><%= e.name %></h5>
                <h5><%= e.ismi %></h5>
            <% }) %>

            
        </div>
      </div>
      <div class="aside-content">
        <div class="project-stats">
          <p class="projects-title">Uyga Vazifa Joylash</p>

            <form action="">
                <input class="input-box12" type="file" name="" id="">
                <input class="input-box12" type="text" name="" id="" placeholder="Description Yozing">
            </form>

        </div>
    </div>
  </div>
    
</body>
</html>